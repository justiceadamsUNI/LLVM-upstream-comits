          },
          {
            "name": "LLVM_ENABLE_PROJECTS",
            "value": "clang",
            "type": "STRING"
          },
          {
            "name": "LLVM_PARALLEL_LINK_JOBS",
            "value": "2",
            "type": "STRING"
          },
          {
            "name": "LLVM_OPTIMIZED_TABLEGEN",
            "value": "ON",
            "type": "BOOL"
          }
        ]
      }
    ]
  }